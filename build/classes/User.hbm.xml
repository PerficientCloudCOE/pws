<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

          <hibernate-mapping>
          <class name="com.cloud.domain.User" table="user_table">
          <id name="id">
          <generator class="assigned"></generator>
          </id>
          
          <property name="userId" column="user_id"></property>
          <property name="firstName" column="first_name"></property>
          <property name="lastName" column="last_name"></property>
          <property name="emailId" column="email_id"></property>
          <property name="role" column="role"></property>
          <property name="creationDate" column="creation_date"></property>
          </class>
          
          </hibernate-mapping>